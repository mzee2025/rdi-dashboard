{
    "dashboard_name": "RDI Dashboard - Research & Data Insights",
    "version": "1.0.0",
    "min_duration": 30,
    "max_duration": 120,
    "start_date": "2025-11-01",
    "target_districts": [
        "Bosaso",
        "Dhusamareb",
        "Beletweyne",
        "Baki",
        "Gabiley"
    ],
    "target_boundaries": {
        "lat_min": -5.0,
        "lat_max": 12.0,
        "lon_min": 40.0,
        "lon_max": 52.0
    },
    "gps_tolerance": 0.01,
    "required_fields": [
        "respondent_information/HH_id",
        "respondent_information/B_hh_size",
        "enums_information/enumerator_name"
    ],
    "column_mapping": {
        "district_column": "respondent_information/District_id",
        "duration_column": "duration_minutes",
        "submission_time_column": "_submission_time",
        "enumerator_column": "enums_information/enumerator_name",
        "geopoint_column": "hh_geopoint"
    },
    "alert_thresholds": {
        "min_completion_rate": 80,
        "max_missing_data_percent": 10,
        "max_duration_flags_percent": 5,
        "max_gps_issues_percent": 2
    },
    "auto_refresh": {
        "enabled": true,
        "interval_hours": 1
    },
    "exports": {
        "include_raw_data": false,
        "export_flagged_records": true,
        "generate_pdf_summary": true
    },
    "branding": {
        "primary_color": "#667eea",
        "secondary_color": "#764ba2",
        "accent_color": "#06A77D",
        "logo_text": "RDI Dashboard"
    }
}
